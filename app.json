{
  "expo": {
    "name": "LaborLink",
    "slug": "portal_empleo_mobile_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons_app.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icons_app.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.portalempleo.mobileapp",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons_app.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "INTERNET"
      ],
      "package": "me.franciscomolina"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "cd3098b3-408f-4a1b-820c-42cb43771521"
      }
    },
    "owner": "francis-1989_molina"
  }
}
